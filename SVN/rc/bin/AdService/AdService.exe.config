<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="cacheConfiguration" type="Adacta.AdInsure.Framework.Core.Cache.CacheConfigurationSection, Adacta.AdInsure.Framework.Core" />
    <section name="Processing" type="Adacta.Service.Utilities.ProcessingConfigurationSectionHandler, Adacta.Service.Utilities" />
    <section name="messaging" type="Adacta.Service.Utilities.MessagingConfigurationSectionHandler, Adacta.Service.Utilities" />
	<section name="loggingServicesConfiguration" type="Adacta.AdInsure.Framework.Core.ServiceLogging.LoggingServiceConfigurationSection, Adacta.AdInsure.Framework.Core" />
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
    <sectionGroup name="common">
      <section name="logging" type="Adacta.AdInsure.Framework.Core.Logging.CommonLoggingConfigurationSectionHandler, Adacta.AdInsure.Framework.Core" />
    </sectionGroup>
  </configSections>
  
  <cacheConfiguration configSource="cacheConfiguration.config"/>
  <loggingServicesConfiguration configSource="conf\LoggingServicesConfiguration.config" />
  <log4net configSource="conf\LoggingConfiguration.config" />

  <common>
    <logging>
      <factoryAdapter type="Common.Logging.Log4Net.Log4NetLoggerFactoryAdapter, Common.Logging.Log4net1215">
        <arg key="configType" value="INLINE" />
      </factoryAdapter>
    </logging>
  </common>
  
  <appSettings file="conf/ImplementationConfiguration.config">
    <!--
	    Če uporabljaš remote Config potem moraš vse nastavitve v sekciji <Settings> prekopirati v remote config 
	    pa mal spremet sintakso, glej AdService.App.config.SAMPLE
	    Izjema je sekcija <RemoteConfig>
	    Če sekcija RemoteConfig ne obstaja, potem se uporablja klasični config 

	    User,Domain,Password nastavi po potrebi, za logon na shared folder. 
	    (Če AdServis ne teče pod uporabniškim imenom ki ima dostop)
	    -->
    <!--<add key="AdInsure:Settings:RemoteConfig:Location" value="AdService.App.config"/>
    <add key="AdInsure:Settings:RemoteConfig:User" value=""/>
    <add key="AdInsure:Settings:RemoteConfig:Domain" value=""/>
    <add key="AdInsure:Settings:RemoteConfig:Passwd" value=""/>-->
    <add key="AdInsure:Settings:Log:LogRoot" value="Logs" />
    <add key="AdInsure:Settings:Log:LogLevel" value="DEBUG" />
    <add key="AdInsure:Settings:Log:TimingThreshold" value="0" />
    <add key="AdInsure:Settings:Log:RemoteLog" value="false"/>

    <!-- Nastavi po potrebi, če se logi pišejo na shared folder.-->
    <!--
            <add key="AdInsure:Settings:Log:User" value=""/>
            <add key="AdInsure:Settings:Log:Domain" value=""/>
            <add key="AdInsure:Settings:Log:Passwd" value=""/>
            -->
    <add key="AdInsure:Settings:Database:Main" value=""/>
    <!--OracleODP-Managed-12-4.0 | System.Data.SqlClient | !Devart.Data.Oracle-->
    <add key="AdInsure:Settings:Database:Provider" value=""/>
    <add key="AdInsure:Settings:Database:Adacta.IIS.Obdelave" value=""/>
    <add key="AdInsure:Settings:DownloadFile:Path" value="D:\WORK\TEST\ADACTA.IIS"/>
    <!--
	        Pravila za nastavljanje dnevnika obdelav:
	        HandlerType : tip vmesnika za vodenje dnevnika obdelav (prednastavljeno : Database)
	        CommentFieldLength : dolzina polja za komentar (prednastavljeno : 120)
	        ProcessNameFieldLength : dolzina polja za opis naziva obdelave (prednastavljeno : 50)
	        LogTableName - naziv tabele za vodenje dnevnika obdelav (prednastavljeno : AD_OBDELAVE)
	        -->
    <add key="AdInsure:Settings:DataLog:LogTableName" value="OB_DNEVNIK" />
    <add key="AdInsure:Settings:DataLog:CommentFieldLength" value="120" />
    <add key="AdInsure:Settings:DataLog:ProcessNameFieldLength" value="50" />
    <add key="AdInsure:Settings:DataLog:HandlerType" value="Database" />
    <!--
	        <add key="AdInsure:Settings:DataLog:LogTableName" value="OB_DNEVNIK" />
	        -->
    <!--
	        Pravila za nastavljanje email naslova administratorja servisa za izvajanje obdelav:
	        ResendMinutes - po koliksnem casu se lahko posljejo iste napake v primeru da se le te ponavljajo.
	        Host - IP ali DNS naslov SMTP streznika za posiljanje email sporoil.
	        From - email naslov poiljatelja napak (servisa).
	        To - email naslov administratorja servise za izvajanje obdelav.
	        -->
    <add key="AdInsure:Settings:AdminMail:Host" value="mail.adacta.si" />
    <add key="AdInsure:Settings:AdminMail:From" value="sender@mail.si" />
    <add key="AdInsure:Settings:AdminMail:To" value="reciver@adacta.si" />
    <!--
	        Pravila za nastavljanje frekvence posiljanja istih napak administratorju:
	        ResendMinutes - po koliksnem asu se lahko poljejo iste napake v primeru da se le te ponavljajo.
	        -->
    <add key="AdInsure:Settings:ErrorMessages:ResendMinutes" value="5" />
    <!--
	        Pravila za nastavljanje urnika obdelav:
	        ScheduleHandler - tip vmesnika za branje urnika in nastavitve obdelav (prednastavljeno - Database)
	        -->
    <add key="AdInsure:Settings:Schedules:ScheduleHandler" value="Database" />
    <!--
	        Pravila za nastavljanje vmesnika za branje zahtevkov rocnih obdelav:
	        OnlineRequestHandler - tip vmesnika za branje zahtevkov rocnih obdelav (prednastavljeno - Database)
	        ProcessingStylePush - nacin branja zahtevkov za rocni zagon - (Domena:true, false) (prednastavljeno - false)
	        pri true - se berejo zahtevki po potrebi (po prihodu zahtevka),
	        pri false - se berejo na OnlineThreadIterationPeriod.
	        -->
    <add key="AdInsure:Settings:OnlineRequests:OnlineRequestHandler" value="Database" />
    <add key="AdInsure:Settings:OnlineRequests:ProcessingStylePush" value="true" />
    <!--
	        Pravila za nastavljanje vmesnika za branje sekvenc (zaenkrat: rocni zagon):
	        HandlerType - tip vmesnika za sekvence (Domena: Database.StoredProcedure*, Database.Table**)
					        * v primeru izbire vmesnika Database.StoredProcedure je potrebno na bazi definirati stored proceduro
					        z nazivom NEXT_ID(in varchar(30) sekvenca, out int outid), ki vraca id vsaj za sekvenco ("OB_ROCNI_ZAGON")
	        TableName ** -  Naziv tabele za sekvence (prednastavljeno: AD_COUNTER), veljavno pri opciji HandlerType = Database.Table
	        IDColumnName ** - Naziv polja za ID sekvence (prednastavljeno COUNTER_STEV), veljavno pri opciji HandlerType = Database.Table
	        SequenceColumnName ** - Naziv polja za Naziv sekvence (prednastavljeno COUNTER_ID), veljavno pri opciji HandlerType = Database.Table
	        -->
    <add key="AdInsure:Settings:Sequences:HandlerType" value="Database.StoredProcedure" />
    <!--
	        <add key="AdInsure:Settings:Sequences:TableName" value="OB_SEKVENCA" />
	        <add key="AdInsure:Settings:Sequences:IDColumnName" value="ID" />
	        <add key="AdInsure:Settings:Sequences:SequenceColumnName" value="SEQ_ID" />
	        -->
    <!--
	        Pravila za nastavljanje casa osvezitve sklopov - Avtomatske obdelave in Rocni zagon
	        OnlineThreadIterationPeriod - nastavitev za osveitev niti za rocni zagon v milisekundah (prednastavljeno 1500)
	        SheduledThreadIterationPeriod - nastavitev za osveitev niti za avtomatske obdelave v milisekundah (prednastavljeno 10000)
	        -->
    <add key="AdInsure:Settings:Processing:Path" value="Processings\Adacta.IIS.Obdelave" />
    <add key="AdInsure:Settings:Processing:OnlineThreadIterationPeriod" value="1500" />
    <add key="AdInsure:Settings:Processing:SheduledThreadIterationPeriod" value="10000" />
    <!--
	        Pravila za nastavljanje remoting dostopa do servisa:
	        DataChannel - tcp/ip port prek katerega so dostopni remotable objekti (prednastavljeno na 8228) />

	        Servis omogoca dostop prek .NET Remoting nacina do naslednjih podatkov:
	        RemoteProcessings - trenutno aktivne niti in obdelave, ki se trenutno izvajajo
	        http://(naslov_streznika):(DataChannel)/RemoteProcessings (?WSDL) (recimo: http://localhost:8228/RemoteProcessings?wsdl)

	        RemoteOnlineRequest - dostop do objekta za narocanje zagona rocnih obdelav in branje statusa izvajanja rocne obdelave
	        http://(naslov_streznika):(DataChannel)/RemoteOnlineRequest (?WSDL) (recimo: http://localhost:8228/RemoteOnlineRequest?wsdl)

	        RemoteDataLogReader - dostop do objekta za branje dnevnika obdelav
	        http://(naslov_streznika):(DataChannel)/RemoteDataLogReader (?WSDL) (recimo: http://localhost:8228/RemoteDataLogReader?wsdl)

	        Kreiranje proxy razredov za dostop odjemalcev se izvede s pomocjo .NET orodja "soapsuds"
	        soapsuds -url:URL_ADDRESS od:.
	        -->
    <add key="AdInsure:Settings:Remoting:DataChannel" value="8228" />
    <!--<add key="AdInsure:Settings:General:AdDataSetCulture" value="en-US"/>-->
    <!-- set DataSet's Culture, value "0" is reserved for InvariantCulture -->
    <add key="AdInsure:Settings:General:ESSearch" value="false" />
	<add key="AdInsure:Settings:General:Language" value="ru-RU" />
    <!--
	  		<add key="AdInsure:Settings:General:ESURL" value="http://localhost:9200"/>
      		<add key="AdInsure:Settings:General:ESIndex" value="coredev_documents"/>
    
			-->
	  <add key="AdInsure:Settings:JsonSchemas:FileStorageRoot" value="JsonSchemas\" />

    <!-- If this flag is set to true, all CSLA requests will be logged in separate log file. -->
    <add key="AdInsure:Settings:General:DataPortalCallTrace" value="false"/>

    <!-- Specif where assemblies and certificate are stored -->
    <add key="AdInsure:Settings:Plugins:FolderPath" value="" />

    <!-- Settings for Active MQ-->
    <add key="AdInsure:MessageBroker:Uri" value="activemq:tcp://localhost:61616" />
    <add key="AdInsure:MessageBroker:Username" value="user" />
    <add key="AdInsure:MessageBroker:Password" value="user" />
  </appSettings>

  <!--
      Pravila za nastavljanje obdelav:
      1) Ce PrivateBinPath ni nastavljen, je prazen (Processing/load[@privateBinPath] ali Processing/appdomain[@privateBinPath]),
      2) V Processing[@basePath] so veljavne samo relativne poti na izvajajoÄŤi exe. In Ĺˇe te so omejene samo na poti niĹľje v drevesu (brez ..),
      3) AppDomain.FriendlyName se v primeru Processing/load izbere iz processCode,

      !!! Pozor - zaradi remote dostopa do internih podatkov poteka obdelav morata obe niti teci v isti aplikacijski domeni..
      -->
  <Processing>
    <appdomain privateBinPath="Processings" friendlyName="Adacta.Service.Processing">
      <load
            codeType="Adacta.Service.Processing"
            processCode="Adacta.Service.Processing.ProcessingThread" />
      <load
            codeType="Adacta.Service.Processing"
            processCode="Adacta.Service.Processing.ProcessingThreadOnline" />
      <!-- Thread that runs message consumers, comment if you don't want this to run -->
      <load 
            codeType="Adacta.Service.Processing"
            processCode="Adacta.Service.Processing.MessageConsumerThread" />
    </appdomain>
  </Processing>

  <messaging configSource="conf\MessagingConfiguration.config" />
  
  <system.data>
    <DbProviderFactories>
      <remove invariant="Devart.Data.Oracle" />
      <add name="dotConnect for Oracle" invariant="Devart.Data.Oracle" description="Devart dotConnect for Oracle" type="Devart.Data.Oracle.OracleProviderFactory, Devart.Data.Oracle, Version=8.5.592.0, Culture=neutral, PublicKeyToken=09af7300eec23701" />
    </DbProviderFactories>
  </system.data>

